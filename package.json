{
  "name": "cere-ddc-sdk",
  "description": "Cere DDC SDK for JavaScript",
  "version": "1.2.8",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --write \"packages/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "compile": "npm run protoc -w ./packages/proto && tsc -b tsconfig.build.json",
    "test": "jest",
    "clean": "rm -rf packages/*/dist/ && rm -rf packages/*/tsconfig.build.tsbuildinfo && rm -rf node_modules && rm -rf packages/*/node_modules",
    "build": "npm install && npm run compile"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "docker-compose": "^0.23.17",
    "jest": "^28.1.0",
    "prettier": "^2.5.1",
    "testcontainers": "^8.10.1",
    "ts-jest": "^28.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@polkadot/dev": "~0.67.5",
    "eslint-plugin-jest": "^26.1.1",
    "ts-loader": "^8.0.18"
  },
  "workspaces": [
    "packages/*"
  ]
}
