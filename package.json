{
  "name": "cere-ddc-sdk",
  "description": "Cere DDC SDK for JavaScript",
  "version": "1.4.4",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --write \"packages/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "push": "node scripts/push.js",
    "compile": "node scripts/compile-proto.mjs",
    "test": "jest --passWithNoTests --silent",
    "clean": "rm -rf packages/*/dist/ && rm -rf packages/*/tsconfig.build.tsbuildinfo && rm -rf node_modules && rm -rf packages/*/node_modules",
    "build": "npm install && npm run compile"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/detect-port": "^1.3.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.0.0",
    "babel-loader": "^8.2.5",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "crypto-browserify": "^3.12.0",
    "docker-compose": "^0.23.17",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^28.1.0",
    "prettier": "^2.5.1",
    "stream-browserify": "^3.0.0",
    "testcontainers": "^8.10.1",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "detect-port": "^1.5.1",
    "tweetnacl": "^1.0.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  }
}
