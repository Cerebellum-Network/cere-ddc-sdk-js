{
  "name": "@cere-ddc-sdk/core",
  "description": "Core with common logic",
  "version": "1.3.1",
  "type": "module",
  "repository": {
    "type": "git",
    "directory": "packages/core",
    "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
  },
  "exports": {
    ".": {
      "browser": "./browser.js",
      "require": "./index.cjs",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./browser": {
      "require": "./browser.cjs",
      "import": "./browser.js",
      "default": "./browser.js"
    }
  },
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "browser"
      ]
    }
  },
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "build:cjs": "TARGET=build:cjs NODE_ENV=production webpack",
    "build:mjs": "TARGET=build:mjs NODE_ENV=production webpack",
    "build:browser": "BROWSERSLIST_ENV=browser npm run build:cjs && BROWSERSLIST_ENV=browser npm run build:mjs",
    "build:node": "BROWSERSLIST_ENV=node npm run build:cjs && BROWSERSLIST_ENV=node npm run build:mjs",
    "build": "npm run build:browser && npm run build:node",
    "push": "node scripts/publish.js",
    "types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "package": "rimraf build && clean-publish --temp-dir build && npm run types && npm run build"
  },
  "dependencies": {
    "@polkadot/extension-dapp": "^0.44.1",
    "@polkadot/extension-inject": "^0.44.1",
    "@polkadot/keyring": "6.11.1",
    "@polkadot/util": "^6.0.5",
    "@polkadot/util-crypto": "6.11.1",
    "@polkadot/wasm-crypto": "4.6.1"
  },
  "devDependencies": {
    "@multiformats/blake2": "^1.0.10",
    "@types/node": "^16.0.0",
    "clean-publish": "^4.0.1",
    "multiformats": "^9.6.5",
    "rimraf": "^3.0.2",
    "typescript": "4.5.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
