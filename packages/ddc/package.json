{
    "name": "@cere-ddc-sdk/ddc",
    "description": "DDC nodes API",
    "version": "2.0.0-rc.7",
    "type": "module",
    "repository": {
        "type": "git",
        "directory": "packages/ddc",
        "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        "browser": "./dist/browser.js",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
    },
    "author": "Cere Network <contact@cere.io (https://www.cere.io/)",
    "license": "Apache-2.0",
    "scripts": {
        "build": "npm-run-all --parallel build:node build:web",
        "build:node": "microbundle --tsconfig tsconfig.node.json --format modern,cjs --target node",
        "build:web": "microbundle --tsconfig tsconfig.web.json --format modern --output dist/browser.js --alias stream/consumers=stream-consumers,stream/web=web-streams-polyfill/ponyfill/es2018",
        "package": "clean-publish",
        "clean": "rimraf dist package"
    },
    "dependencies": {
        "@cere-ddc-sdk/blockchain": "2.0.0-rc.7",
        "@improbable-eng/grpc-web": "^0.15.0",
        "@protobuf-ts/grpc-transport": "^2.9.1",
        "@protobuf-ts/runtime": "^2.9.1",
        "rfc4648": "^1.5.3"
    },
    "devDependencies": {
        "@protobuf-ts/plugin": "^2.9.1",
        "web-streams-polyfill": "^3.2.1",
        "stream-consumers": "^1.0.2"
    },
    "peerDependencies": {
        "@grpc/grpc-js": "^1.6.0",
        "@polkadot/keyring": "^10.1.8",
        "@polkadot/types": "^9.4.1",
        "@polkadot/util-crypto": "^10.1.8"
    },
    "peerDependenciesMeta": {
        "@grpc/grpc-js": {
            "optional": true
        },
        "@polkadot/keyring": {
            "optional": true
        },
        "@polkadot/types": {
            "optional": true
        },
        "@polkadot/util-crypto": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
