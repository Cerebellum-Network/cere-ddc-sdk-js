{
    "name": "@cere-ddc-sdk/ddc",
    "description": "DDC nodes API",
    "version": "2.0.0-rc.6",
    "type": "module",
    "repository": {
        "type": "git",
        "directory": "packages/ddc",
        "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
    },
    "main": "./dist/index.cjs",
    "types": "./dist/types",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "author": "Cere Network <contact@cere.io (https://www.cere.io/)",
    "license": "Apache-2.0",
    "scripts": {
        "compile": "protoc --ts_opt long_type_number --ts_out ./src/grpc --proto_path protos ./protos/*.proto",
        "build:cjs": "TARGET=build:cjs NODE_ENV=production webpack",
        "build:mjs": "TARGET=build:mjs NODE_ENV=production webpack",
        "build:node": "BROWSERSLIST_ENV=node npm run build:cjs && BROWSERSLIST_ENV=node npm run build:mjs",
        "build": "npm run clean && npm run types && npm run build:node",
        "types": "tsc -p tsconfig.build.json --emitDeclarationOnly || exit 0",
        "package": "clean-publish",
        "clean": "rimraf dist package"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.6.0",
        "@protobuf-ts/grpc-transport": "^2.9.1",
        "@protobuf-ts/grpcweb-transport": "^2.9.1",
        "@protobuf-ts/runtime": "^2.9.1",
        "rfc4648": "^1.5.3",
        "@cere-ddc-sdk/blockchain": "*"
    },
    "devDependencies": {
        "@protobuf-ts/plugin": "^2.9.1"
    },
    "peerDependencies": {
        "@polkadot/keyring": "^10.1.8",
        "@polkadot/types": "^9.4.1",
        "@polkadot/util-crypto": "^10.1.8"
    },
    "peerDependenciesMeta": {
        "@polkadot/keyring": {
            "optional": true
        },
        "@polkadot/types": {
            "optional": true
        },
        "@polkadot/util-crypto": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
