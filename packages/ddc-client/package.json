{
    "name": "@cere-ddc-sdk/ddc-client",
    "description": "DDC client",
    "version": "1.9.5",
    "type": "module",
    "repository": {
        "type": "git",
        "directory": "packages/ddc-client",
        "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
    },
    "main": "./dist/index.cjs",
    "types": "./dist/types",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "author": "Cere Network <contact@cere.io (https://www.cere.io/)",
    "license": "Apache-2.0",
    "scripts": {
        "build:cjs": "TARGET=build:cjs NODE_ENV=production webpack",
        "build:mjs": "TARGET=build:mjs NODE_ENV=production webpack",
        "build:node": "BROWSERSLIST_ENV=node npm run build:cjs && BROWSERSLIST_ENV=node npm run build:mjs",
        "build": "npm run clean && npm run types && npm run build:node",
        "types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
        "package": "clean-publish",
        "clean": "rimraf dist package"
    },
    "dependencies": {
        "@cere-ddc-sdk/core": "1.9.5",
        "@cere-ddc-sdk/ddc": "1.9.5",
        "@cere-ddc-sdk/file-storage": "1.9.5",
        "@cere-ddc-sdk/smart-contract": "1.9.5"
    },
    "devDependencies": {
        "@types/node": "^16.0.0",
        "buffer": "^6.0.3"
    }
}
