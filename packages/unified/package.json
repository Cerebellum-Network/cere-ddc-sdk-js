{
  "name": "@cere-ddc-sdk/unified",
  "description": "Unified Data Ingestion SDK - Single entry point for all data ingestion operations",
  "version": "2.14.1",
  "type": "module",
  "repository": {
    "type": "git",
    "directory": "packages/unified",
    "url": "git+https://github.com/Cerebellum-Network/cere-ddc-sdk-js.git"
  },
  "main": "dist/index.cjs",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "require": "./dist/index.cjs",
    "browser": "./dist/browser.js",
    "default": "./dist/index.js"
  },
  "author": "Cere Network <contact@cere.io (https://www.cere.io/)",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm-run-all --parallel build:node build:web",
    "build:node": "microbundle --tsconfig tsconfig.build.json --format esm,cjs --target node",
    "build:web": "microbundle --tsconfig tsconfig.build.json --format modern --output dist/browser.js",
    "package": "clean-publish",
    "clean": "rimraf dist package",
    "docs": "typedoc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@cere-activity-sdk/ciphers": "^0.1.7",
    "@cere-activity-sdk/events": "^0.1.7",
    "@cere-activity-sdk/signers": "^0.1.7",
    "@cere-ddc-sdk/ddc-client": "2.14.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
